{
  "extends": "@tsconfig/node12/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "incremental": true,
    "inlineSources": true,
    "module": "ES6",
    "moduleResolution": "node",
    "newLine": "lf",
    "noUnusedLocals": true,
    "outDir": "dist",
    "preserveConstEnums": true,
    "sourceMap": true,
    "stripInternal": true
  },
  "include": [
    "src/index.ts"
  ]
}
